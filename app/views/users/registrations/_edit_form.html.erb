  <%= form_with model: @user, url: user_registration_path,
                id: "edit_user", class: "edit_user", local: true do |f| %>
    <%= render "shared/error_messages", object: f.object %>
    <% if session[:forwarding_path] == edit_user_registration_path %>
      <%= render "edit_profile_form",  f: f %>
    <% else %>
      <%= render "edit_password_form", f: f %>
    <% end %>
    <%= f.submit "更新" %>
  <% end %>