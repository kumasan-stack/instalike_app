<% if @user.errors.any? %>
  <dev id="error_explanation">
    <dev class="alert alert-danger">
      The form contains <%= pluralize @user.errors.count, "error" %>.
    </dev>
    <ul>
      <% @user.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
      <% end %>
    </ul>
  </dev>
<% end %>